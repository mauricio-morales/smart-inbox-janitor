{
  "name": "smart-inbox-janitor",
  "version": "0.1.0",
  "description": "AI-powered email triage assistant for Gmail with provider-agnostic architecture",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit --strict",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:types": "jest __tests__/types/",
    "test:schemas": "jest __tests__/schemas/",
    "test:factories": "jest __tests__/factories/",
    "preview": "electron-vite preview",
    "clean": "rm -rf dist dist-electron coverage",
    "prepack": "npm run build",
    "_comment_ci": "=== LOCAL CI/CD VALIDATION SCRIPTS ===",
    "ci:check": "npm run ci:clean && npm run ci:quality && npm run ci:build && npm run ci:security && echo '✅ All CI/CD checks passed!'",
    "ci:quick": "npm run lint && npm run type-check && npm run build",
    "ci:quality": "npm run lint && npm run type-check && npm run format:check && npm run test",
    "ci:build": "npm run build && npm run build:electron:all",
    "ci:security": "npm audit --audit-level=moderate && npm run ci:deps-check",
    "ci:package": "npm run ci:deps-check && npm run ci:outdated",
    "ci:clean": "npm run clean && rm -rf node_modules && npm ci",
    "ci:deps-check": "npm ls --depth=0 || echo '⚠️  Some dependencies may have issues'",
    "ci:outdated": "npm outdated || echo '📦 Dependency status checked'",
    "_comment_build": "=== ELECTRON BUILD VARIANTS ===",
    "build:electron:all": "npm run build:electron:mac && npm run build:electron:win && npm run build:electron:linux",
    "build:electron:mac": "electron-builder --mac --publish=never",
    "build:electron:win": "electron-builder --win --publish=never",
    "build:electron:linux": "electron-builder --linux --publish=never",
    "build:electron:current": "electron-builder --publish=never"
  },
  "keywords": [
    "email",
    "gmail",
    "ai",
    "triage",
    "electron",
    "typescript",
    "inbox",
    "automation"
  ],
  "author": "Smart Inbox Janitor Contributors",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.5",
    "better-sqlite3": "^9.6.0",
    "googleapis": "^133.0.0",
    "keytar": "^7.9.0",
    "openai": "^4.28.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "build": {
    "appId": "com.smartinboxjanitor.app",
    "productName": "Smart Inbox Janitor",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
