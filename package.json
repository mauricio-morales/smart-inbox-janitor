{
  "name": "smart-inbox-janitor",
  "version": "0.1.0",
  "description": "AI-powered email triage assistant for Gmail with provider-agnostic architecture",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit --strict",
    "format": "prettier . --write",
    "format:check": "prettier . -c",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false --passWithNoTests",
    "test:types": "jest __tests__/types/",
    "test:schemas": "jest __tests__/schemas/",
    "test:factories": "jest __tests__/factories/",
    "preview": "electron-vite preview",
    "clean": "rm -rf dist dist-electron coverage",
    "prepack": "npm run build",
    "_comment_console": "=== CONSOLE MODE PROVIDER VALIDATION SCRIPTS ===",
    "debug:providers": "electron . --console --verbose",
    "debug:providers:json": "electron . --console --format=json",
    "debug:providers:table": "electron . --console --format=table --verbose",
    "debug:gmail": "electron . --console --providers=gmail --verbose",
    "debug:openai": "electron . --console --providers=openai --verbose",
    "debug:storage": "electron . --console --providers=storage --verbose",
    "validate:console": "electron . --console --timeout=60 --exit-on-failure",
    "validate:console:retry": "electron . --console --retry --retry-attempts=3 --verbose",
    "test:providers": "npm run debug:providers:json | jq 'select(.type == \"validation_summary\")' || npm run debug:providers:json",
    "test:providers:ci": "electron . --headless --format=json --timeout=30",
    "_comment_ci": "=== LOCAL CI/CD VALIDATION SCRIPTS ===",
    "ci:check": "npm run ci:clean && npm run ci:quality && npm run ci:build && npm run ci:security && echo '✅ All CI/CD checks passed!'",
    "ci:quick": "npm run lint && npm run type-check && npm run build",
    "ci:quality": "npm run lint && npm run type-check && npm run format:check && npm run test",
    "ci:build": "npm run build && npm run build:electron:all",
    "ci:security": "npm audit --audit-level=moderate && npm run ci:deps-check",
    "ci:package": "npm run ci:deps-check && npm run ci:outdated",
    "ci:clean": "npm run clean && rm -rf node_modules && npm ci",
    "ci:deps-check": "npm ls --depth=0 || echo '⚠️  Some dependencies may have issues'",
    "ci:outdated": "npm outdated || echo '📦 Dependency status checked'",
    "_comment_build": "=== ELECTRON BUILD VARIANTS ===",
    "build:electron:all": "npm run build:electron:mac && npm run build:electron:win && npm run build:electron:linux",
    "build:electron:mac": "electron-builder --mac --publish=never",
    "build:electron:win": "electron-builder --win --publish=never",
    "build:electron:linux": "electron-builder --linux --publish=never",
    "build:electron:current": "electron-builder --publish=never",
    "prepare": "husky"
  },
  "keywords": [
    "email",
    "gmail",
    "ai",
    "triage",
    "electron",
    "typescript",
    "inbox",
    "automation"
  ],
  "author": "Smart Inbox Janitor Contributors",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "electron-vite": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^7.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml,css,scss,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.5",
    "@types/lru-cache": "^7.10.9",
    "@xstate/react": "^6.0.0",
    "better-sqlite3": "^9.6.0",
    "cli-table3": "^0.6.5",
    "dotenv": "^17.2.1",
    "googleapis": "^133.0.0",
    "keytar": "^7.9.0",
    "lru-cache": "^10.4.3",
    "openai": "^4.28.4",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "xstate": "^5.20.2",
    "yargs": "^18.0.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "build": {
    "appId": "com.smartinboxjanitor.app",
    "productName": "Smart Inbox Janitor",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
