<ResourceDictionary xmlns="https://github.com/avaloniaui"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- Professional Typography System -->
    <!-- Based on Inter font family for maximum readability and professional appearance -->

    <!-- Display Typography (Large Headlines) -->
    <Style x:Key="DisplayLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="32" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="LineHeight" Value="40" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,24" />
    </Style>

    <Style x:Key="DisplayMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="28" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="LineHeight" Value="36" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,20" />
    </Style>

    <!-- Headline Typography -->
    <Style x:Key="HeadlineLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="24" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="LineHeight" Value="32" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,16" />
    </Style>

    <Style x:Key="HeadlineMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="20" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="LineHeight" Value="28" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,12" />
    </Style>

    <Style x:Key="HeadlineSmall" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="18" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="LineHeight" Value="24" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,12" />
    </Style>

    <!-- Title Typography -->
    <Style x:Key="TitleLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="18" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="24" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,8" />
    </Style>

    <Style x:Key="TitleMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="22" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,6" />
    </Style>

    <Style x:Key="TitleSmall" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="20" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="Margin" Value="0,0,0,4" />
    </Style>

    <!-- Body Typography -->
    <Style x:Key="BodyLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="24" />
        <Setter Property="Foreground" Value="{DynamicResource TextSecondary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="BodyMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="20" />
        <Setter Property="Foreground" Value="{DynamicResource TextSecondary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="BodySmall" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <!-- Label Typography -->
    <Style x:Key="LabelLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="14" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="20" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
    </Style>

    <Style x:Key="LabelMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
    </Style>

    <Style x:Key="LabelSmall" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="14" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
    </Style>

    <!-- Caption Typography -->
    <Style x:Key="CaptionLarge" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="CaptionMedium" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="14" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="CaptionSmall" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="10" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="12" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <!-- Special Purpose Typography -->
    
    <!-- App Title Style (for main application branding) -->
    <Style x:Key="AppTitle" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="24" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="LineHeight" Value="32" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
    </Style>

    <!-- Provider Name Style (for provider cards) -->
    <Style x:Key="ProviderName" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="LineHeight" Value="22" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
    </Style>

    <!-- Provider Description Style -->
    <Style x:Key="ProviderDescription" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource TextTertiary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="MaxLines" Value="2" />
    </Style>

    <!-- Status Text Style -->
    <Style x:Key="StatusText" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="18" />
        <Setter Property="Foreground" Value="{DynamicResource TextPrimary}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <!-- Button Text Styles -->
    <Style x:Key="ButtonTextPrimary" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="18" />
        <Setter Property="Foreground" Value="{DynamicResource TextOnAccent}" />
    </Style>

    <Style x:Key="ButtonTextSecondary" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="LineHeight" Value="18" />
        <Setter Property="Foreground" Value="{DynamicResource AccentBlue}" />
    </Style>

    <!-- Error Message Style -->
    <Style x:Key="ErrorMessage" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource StatusError}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <!-- Success Message Style -->
    <Style x:Key="SuccessMessage" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource StatusSuccess}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <!-- Warning Message Style -->
    <Style x:Key="WarningMessage" Selector="TextBlock">
        <Setter Property="FontFamily" Value="Inter, Segoe UI, sans-serif" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="Regular" />
        <Setter Property="LineHeight" Value="16" />
        <Setter Property="Foreground" Value="{DynamicResource StatusWarning}" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

</ResourceDictionary>